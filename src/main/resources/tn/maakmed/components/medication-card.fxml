<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<fx:root minHeight="70.0" minWidth="200.0" type="VBox" xmlns="http://javafx.com/javafx/21.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <HBox alignment="TOP_CENTER" spacing="20.0">
            <children>
                <Label fx:id="name" text="Name" underline="true" />
                <Text fx:id="description" strokeType="OUTSIDE" strokeWidth="0.0" text="Description" />
            </children>
        </HBox>
        <HBox alignment="TOP_CENTER" VBox.vgrow="ALWAYS">
            <children>
                <CheckBox fx:id="insured" mnemonicParsing="false" text="Insured" />
            </children>
        </HBox>
    </children>
</fx:root>
